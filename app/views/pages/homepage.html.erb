<div id="compatible_ram">
<h1>Motherboard</h1>
<script src="http://ajax.googleapis.com/ajax/libs/jquery/2.0.0/jquery.min.js"></script>

<table style="width: 100%">
  <tr>
    <th align="left">Model Name</th>
    <th align="left">Manufacturer</th>
    <th align="left">Ram Frequency</th>
    <th align="left">Ram Generation</th>
  </tr>
<% @motherboards.each do |c| %>
<tbody>
  <tr>
    <td><%= link_to c.Manufacturer, {controller: "pages", :action => "compatibles", :data => c}, remote: true%></td>
    <td><%= c.ModelName %></td>
    <td><%= c.RamFrequency %></td>
    <td><%= c.RamGeneration %></td>
  </tr>
</tbody>
<% end %>
</table>
  <span></span>
</div>



